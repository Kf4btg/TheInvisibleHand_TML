[
  {
    name: Placeable
    requires: [
      Property create_tile | create_wall
      Property.Ident !wand
    ]
  }

  {
    name: Weapon
    requires: [
      Property damage
      Property !not_ammo | UseStyle use_style_any
    ]
  }

  {
    name: Armor
    requires: [
      Property !vanity
      EquipSlot head | body | leg
    ]
  }

  {
    name: Tool
    requires: [
      Tool tool_any
    ]
  }

  {
    name: Ammo
    requires: [
      Property.Ident ammo
    ]
  }
]
